{
	"name": "j7-dev/wp-utils",
	"version": "0.3.69",
	"description": "Various WordPress classes and traits for WordPress",
	"homepage": "https://github.com/j7-dev",
	"type": "library",
	"keywords": [
		"wordpress",
		"utils",
		"traits",
		"classes"
	],
	"license": "GPL-3.0-or-later",
	"autoload": {
		"psr-4": {
			"J7\\WpUtils\\Classes\\": "src/classes/",
			"J7\\WpUtils\\Traits\\": "src/traits/"
		}
	},
	"authors": [
		{
			"name": "JerryLiu",
			"email": "j7.dev.gg@gmail.com",
			"homepage": "https://github.com/j7-dev",
			"role": "Developer"
		}
	],
	"minimum-stability": "dev",
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"phpstan/extension-installer": false
		}
	},
	"require": {
		"j7-dev/tgm-plugin-activation-forked": "1.2.5",
		"yahnis-elsts/plugin-update-checker": "5.3",
		"j7-dev/wp-plugin-trait": "0.2.10"
	},
	"require-dev": {
      "squizlabs/php_codesniffer": "@stable",
      "wp-coding-standards/wpcs": "@stable",
      "dealerdirect/phpcodesniffer-composer-installer": "@stable",
      "phpcompatibility/php-compatibility": "@stable",
      "phpstan/phpstan": "@stable",
      "php-stubs/woocommerce-stubs": "@stable",
      "php-stubs/wordpress-stubs": "@stable",
      "phpstan/extension-installer": "@stable"
	},
	"scripts": {
		"lint": "phpcs"
	}
}